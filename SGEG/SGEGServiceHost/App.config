<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.serviceModel>
    <services>
      <service name="SGEGService.SGEGService" behaviorConfiguration="mexBehavior">
        
        <!-- Web Clients -->
        <endpoint address="SGEGService" binding="basicHttpBinding" contract="SGEGService.ISGEGPublicService">
        </endpoint>
        <!-- ########### -->

        <!-- Local Clients -->
        <endpoint address="SGEGService" binding="netTcpBinding" contract="SGEGService.ISGEGPrivateService">
        </endpoint>
        <!-- ########### -->
        
        <!-- Metadata -->
        <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange">
        </endpoint>
        <!-- ########### -->
        
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8888/"/>
            <add baseAddress="net.tcp://localhost:8890/"/>
            <!-- Ajouter l'adresse des nouveaux clients ici! -->
            
          </baseAddresses>
        </host>
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior name="mexBehavior">
          <serviceMetadata httpGetEnabled="true"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
</configuration>